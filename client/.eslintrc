{
    "extends": [ "react-app", "react-app/jest" ], 
      "rules": {
        "no-console": "warn",
        "prefer-const": "error",
        "quotes": [ "error", "single" ],
        "jsx-quotes": [ "error", "prefer-double" ],
        "indent": [ "warn", 2 ],
        "comma-dangle": [ "error", "always-multiline" ],
        "semi": [ "warn", "never" ],
        "import/order": [
            "error",
            {
                "groups": [ "builtin", "external", "internal", "parent", "sibling", "index", "object", "type"],
                "newlines-between": "always"
            }
        ]
      }
}